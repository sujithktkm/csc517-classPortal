<%= bootstrap_form_for(@Admin, url: admins_edit_admin_save_path) do |f| %>
    <%= f.text_field :name, value: params[:name] %>
    <%= f.email_field :email, value: params[:email] %>
    <%= f.password_field :password %>
    <%= f.submit 'Update', :class => 'btn btn-success btn-submit' %>&nbsp
    <%= link_to 'Back', :back, :class => 'btn btn-info' %>
<% end %>