<%= bootstrap_form_for(@Course,:url => url_for(:controller => 'admins', :action => 'edit_admin_course_save')) do |f| %>
    <%= f.text_field :coursenumber %>
    <%= f.text_field :title%>
    <%= f.text_field :description%>
    <%= f.date_select :start_date %>
    <%= f.date_select :end_date%>
    <%= f.select(:status, options_for_select([['Active', true], ['InActive', false]]), {}, {class: 'form-control'}) %>
    <%= f.collection_select :instructor_id, Instructor.where("is_activeuser = ?",true), :id, :name, include_blank: false %>
    <%= f.submit 'Edit course', :class => 'btn btn-info' %>
    <%= link_to 'Back', admins_manage_course_path, :class => 'btn btn-info' %>

<% end %>