<%= bootstrap_form_for(@Instructor,:url => url_for(:controller => 'admins', :action => 'create_instructor_save')) do |f| %>
    <%= f.text_field :name, :placeholder => 'Name' %>
    <%= f.email_field :email, :placeholder => 'Email' %>
    <%= f.password_field :password, :placeholder => 'Password' %>
    <%= f.submit 'Create Instructor', :class => 'btn btn-info' %>
    <%= link_to 'Back', admins_manage_user_path, :class => 'btn btn-info' %>

<% end %>
