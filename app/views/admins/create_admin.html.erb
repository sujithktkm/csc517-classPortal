<%= bootstrap_form_for(@New_admin, :url => url_for(:controller => 'admins', :action => 'create')) do |f| %>
    <%= f.text_field :name, :placeholder => 'Name' %>
    <%= f.email_field :email, :placeholder => 'Email' %>
    <%= f.password_field :password, :placeholder => 'Password' %>
    <%= f.submit 'Create Admin', :class => 'btn btn-info' %>
    <%= link_to 'Back', admins_manage_admin_path, :class => 'btn btn-info' %>



<% end %>
